<!-- EXPLORER/PROJECTS COLUMN -->
<div class="column" id="projects_column">
	<div class="full">
		<table class="full header">
			<tr>
				<td class="parent_name">Proyectos</td>
				<td></td>
			</tr>
		</table>
	</div>
	
	<!-- Por cada hijo de la variable parent, se hace render de un item de la columna -->
	<div class="full children">
		<% @projects.each do |project| %>
			<%= render 'explorer/project_item', project: project %>
		<% end %>
	</div>
</div>		