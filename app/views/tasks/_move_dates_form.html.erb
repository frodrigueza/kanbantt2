<%= bootstrap_form_tag url: task_move_dates_path(@task), remote: true do |f|  %>
	
	<%= f.date_select :new_date, {default: @task.expected_start_date_from_children ,:use_month_names => ['Enero', 'Febrero', 'Marzo', 'Abril', 'Mayo', 'Junio', 'Julio', 'Agosto', 'Septiembre', 'Octubre', 'Noviembre','Diciembre'], :required => true, label: 'Nueva fecha de inicio'} %>
	<%= f.submit 'Enviar' %>
	
<% end %>