<%= bootstrap_form_tag url: task_move_dates_path(@task) do |f|  %>
	
	<%= f.text_field :new_date, :class => 'datepicker', label:'Nueva fecha de inicio', value: @task.expected_start_date_from_children.to_date %>
	
	<%= f.form_group do %>
		<%= f.submit 'Enviar', class:'btn btn-primary' %>
	<% end %>
	
<% end %>