<h1>Listing resources_reports</h1>

<table>
  <thead>
    <tr>
      <th>Task</th>
      <th>Resources</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @resources_reports.each do |resources_report| %>
      <tr>
        <td><%= resources_report.task_id %></td>
        <td><%= resources_report.resources %></td>
        <td><%= resources_report.user_id %></td>
        <td><%= link_to 'Show', resources_report %></td>
        <td><%= link_to 'Edit', edit_resources_report_path(resources_report) %></td>
        <td><%= link_to 'Destroy', resources_report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Resources report', new_resources_report_path %>
