<div class="row">
	<div id="resources_chart" class="col-md-12">

	</div>
</div>
<div class="row">
	<div class="col-md-3">
		<div class="row">
			<div class="col-md-12">
				Avance
			</div>
			<div class="col-md-12">
				<%= @project.real_progress_function(Date.today, true) %>
			</div>
		</div>
	</div>

	<div class="col-md-3">
		<div class="row">
			<div class="col-md-12">
