<div class="row">
	<div id="resources_chart">

	</div>
</div>

<div class="row">
	<div class="col-md-3 indicator-footer" id="real_progress">
		<div class="row">
			<div class="col-md-12">
				Avance
			</div>
			<div class="col-md-12">
				<%= @project.real_progress %> %
			</div>
		</div>
	</div>

	<div class="col-md-3 indicator-footer <%= project_delta_progress_css(@project, false) %>">
		<div class="row">
			<div class="col-md-12">
				Diferencia
			</div>
			<div class="col-md-12">
				<%= @project.progress_delta(true) %> %
			</div>
		</div>
	</div>

	<div class="col-md-3 indicator-footer" id="expected_progress">
		<div class="row">
			<div class="col-md-12">
				Avance esperado
			</div>
			<div class="col-md-12">
				<%= @project.expected_progress %> %
			</div>
		</div>
	</div>

	<div class="col-md-3 indicator-footer <%= project_delayed_days_progress_css(@project, false) %>">
		<div class="row">
			<div class="col-md-12">
				<%= @project.f_delayed_or_advanced_days(true)[0] %>
			</div>
			<div class="col-md-12">
				<%= @project.f_delayed_or_advanced_days(true)[1] %>
			</div>
		</div>
	</div>
</div>